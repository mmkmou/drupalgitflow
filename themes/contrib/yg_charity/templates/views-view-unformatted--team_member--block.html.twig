<section id="team" class="text-center">
  <div class="container">
    <div class="col-md-12 section-title">
      <h2>TEAM</h2>
      <div class="title-img"><img src="{{ url('<front>') }}/themes/yg_charity/images/line.gif" class="img-responsive" alt="title"></div>
    </div>
    <div class="row">
      {% for row in rows %}
     {%
       set row_classes = [
         default_row_class ? 'views-row',
       ]
     %}
     <div {{ row.attributes.addClass(row_classes) }}>
       {{ row.content }}
     </div>
    {% endfor %}
    </div>
  </div>
</section>
